.org 0x3000
SET PC, echof

; 0x42c jumptable
:os_enterdone dat 0x1002
:os_input dat 0x1004

:echo dat "echo", 0

:echof
	jsr [kern_newline]
	set a, echo
	jsr [kern_strlen]
	add a, 1	        ; +1 to kern_strlen,
			            ; for the space

	set b, [os_input]
	set b, [b]
	add b, a 	        ; start at `kern_strlen'
	set a, b	        ; string pointer
	jsr [kern_printnl]

	set a, 1
	set pc, [os_enterdone]
	
.include "jumptable.dasm16"
